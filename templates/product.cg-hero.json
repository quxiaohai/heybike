    "cg_product_feature_list_WxXtr9": {
      "type": "cg_product-feature-list",
      "blocks": {
        "feature_9b3zEb": {
          "type": "feature",
          "settings": {
            "feature_img": "shopify://shop_images/E4_da73ffd7-99b0-49cc-b7a7-b5666ec3c990.jpg",
            "feature_heading": "Built-in Display and Headlight",
            "feature_desc": "<p>Stay informed and safe with the integrated handlebar, displaying all your riding essentials.Displays riding information conveniently. Improves safety with integrated headlight. Enhances aesthetics with sleek design.</p>"
          }
        },
        "feature_ybC4Dj": {
          "type": "feature",
          "settings": {
            "feature_img": "shopify://shop_images/E5_a54ee130-e25c-4745-bd7f-0ef75e5ede1a.jpg",
            "feature_heading": "Range up to 60 Miles",
            "feature_desc": "<p>Embark on thrilling adventures and push your limits with HERO's remarkable range of up to 60 miles on a single charge. Powered by our 864Wh battery, rigorously certified with UL2849, HERO ensures worry-free exploration.</p>"
          }
        },
        "feature_fVDXQH": {
          "type": "feature",
          "settings": {
            "feature_img": "shopify://shop_images/E6_cdf704a5-5d3d-4b96-95d5-3f587382e25c.jpg",
            "feature_heading": "26\"x4\" Fat Tires",
            "feature_desc": "<p>Cruise confidently with two fat tires offering unmatched stability and traction on any terrain.</p>"
          }
        },
        "feature_mzDqYy": {
          "type": "feature",
          "settings": {
            "feature_img": "shopify://shop_images/E8_5bf1a105-8a8c-401e-bc71-09dcc162a244.jpg",
            "feature_heading": "Torque Sensor",
            "feature_desc": "<p>Utilizing torque sensing pedal assist ensures a beautifully responsive ride feels and there's virtually zero latency. Translate your every movement into smooth motor power.</p>"
          }
        },
        "feature_hHWBPT": {
          "type": "feature",
          "settings": {
            "feature_img": "shopify://shop_images/E7_bf6612e8-14fc-44f8-99d5-caa6b829c8af.jpg",
            "feature_heading": "Shock-Absorbing Saddle",
            "feature_desc": "<p>Ride in comfort with a saddle that absorbs shocks, minimizing impact on your body.</p>"
          }
        }
