  <div class="ai-video-popup-{{ ai_gen_id }}">
    <div class="ai-video-popup-content-{{ ai_gen_id }}">
      <button class="ai-video-popup-close-{{ ai_gen_id }}" aria-label="Close video">×</button>
      <iframe class="ai-video-popup-iframe-{{ ai_gen_id }}" src="" allowfullscreen></iframe>
    </div>
  </div>
</product-video-navigation-{{ ai_gen_id }}><script>
(function() {
  class ProductVideoNavigation{{ ai_gen_id }} extends HTMLElement {
    constructor() {
      super();
      this.tabs = this.querySelectorAll('.ai-video-nav-tab-{{ ai_gen_id }}');
      this.sections = this.querySelectorAll('.ai-video-section-{{ ai_gen_id }}');
      this.videoThumbnails = this.querySelectorAll('.ai-video-thumbnail-{{ ai_gen_id }}');
      this.popup = this.querySelector('.ai-video-popup-{{ ai_gen_id }}');
      this.popupIframe = this.querySelector('.ai-video-popup-iframe-{{ ai_gen_id }}');
      this.popupClose = this.querySelector('.ai-video-popup-close-{{ ai_gen_id }}');
    }

    connectedCallback() {
      this.setupTabNavigation();
      this.setupVideoPopup();
      this.setupScrollSpy();
    }

    setupTabNavigation() {
      this.tabs.forEach(tab => {
        tab.addEventListener('click', () => {
          const sectionId = tab.dataset.section + '-{{ ai_gen_id }}';
          const targetSection = document.getElementById(sectionId);
          
          if (targetSection) {
            targetSection.scrollIntoView({
              behavior: 'smooth',
              block: 'start'
            });
          }
        });
      });
    }

    setupScrollSpy() {
      const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
          if (entry.isIntersecting) {
            const sectionId = entry.target.id.replace('-{{ ai_gen_id }}', '');
            this.updateActiveTab(sectionId);
          }
        });
      }, {
        threshold: 0.3,
        rootMargin: '-100px 0px -50% 0px'
      });

      this.sections.forEach(section => {
        observer.observe(section);
      });
    }

    updateActiveTab(sectionId) {
      this.tabs.forEach(tab => {
        tab.classList.remove('active');if (tab.dataset.section === sectionId) {
          tab.classList.add('active');
        }
      });
    }

    setupVideoPopup() {
      this.videoThumbnails.forEach(thumbnail => {
        thumbnail.addEventListener('click', () => {
          const videoId = thumbnail.dataset.videoId;
          if (videoId) {
            this.openVideoPopup(videoId);
          }
        });
      });

      this.popupClose.addEventListener('click', () => {
        this.closeVideoPopup();
      });

      this.popup.addEventListener('click', (e) => {
        if (e.target === this.popup) {
          this.closeVideoPopup();
        }
      });document.addEventListener('keydown', (e) => {
        if (e.key === 'Escape' && this.popup.classList.contains('active')) {
          this.closeVideoPopup();
        }
      });
    }

    openVideoPopup(videoId) {
      const embedUrl = `https://www.youtube.com/embed/${videoId}?autoplay=1&rel=0`;
      this.popupIframe.src = embedUrl;
      this.popup.classList.add('active');document.body.style.overflow = 'hidden';
    }

    closeVideoPopup() {
      this.popup.classList.remove('active');
      this.popupIframe.src = '';
      document.body.style.overflow = '';
    }
  }

  customElements.define('product-video-navigation-{{ ai_gen_id }}', ProductVideoNavigation{{ ai_gen_id }});
})();
</script>

