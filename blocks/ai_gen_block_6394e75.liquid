{% doc %}
  @prompt
    Product video navigation with scrollable tabs for Ranger 3.0 Pro, Mars 3.0, Ranger S, Mars 2.0, ALPHA, Cityrun, Horizon. All sections stay visible, smooth scroll to sections. Each section has title and View Product button. 6 YouTube videos per section in 3x2 grid. Video blocks have thumbnail with play button, title field, creator name field. Videos open in popup. Low z-index for navigation so header menus appear above.

{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-video-nav-container-{{ ai_gen_id }} {
    width: 100%;
    position: relative;
  }

  .ai-video-nav-tabs-{{ ai_gen_id }} {
    position: sticky;
    top: 0;
    z-index: 10;
    background-color: {{ block.settings.nav_background_color }};
    border-bottom: 1px solid {{ block.settings.nav_border_color }};
    overflow-x: auto;
    scrollbar-width: none;
    -ms-overflow-style: none;
  }

  .ai-video-nav-tabs-{{ ai_gen_id }}::-webkit-scrollbar {
    display: none;
  }

  .ai-video-nav-tabs-list-{{ ai_gen_id }} {
    display: flex;
    min-width: max-content;
    padding: 0 20px;
    margin: 0;
    list-style: none;}

  .ai-video-nav-tab-{{ ai_gen_id }} {
    flex-shrink: 0;
    padding: 16px 24px;
    background: none;
    border: none;
    color: {{ block.settings.nav_text_color }};
    font-size: {{ block.settings.nav_font_size }}px;
    font-weight: 500;
    cursor: pointer;
    transition: all 0.3s ease;
    border-bottom: 3px solid transparent;
    white-space: nowrap;
  }

  .ai-video-nav-tab-{{ ai_gen_id }}:hover {
    color: {{ block.settings.nav_active_color }};
  }

  .ai-video-nav-tab-{{ ai_gen_id }}.active {
    color: {{ block.settings.nav_active_color }};
    border-bottom-color: {{ block.settings.nav_active_color }};
  }

  .ai-video-sections-{{ ai_gen_id }} {
    padding: 40px 20px;
    max-width: {{ settings.page_width }}px;
    margin: 0 auto;
  }

  .ai-video-section-{{ ai_gen_id }} {
    margin-bottom: 80px;
    scroll-margin-top: 100px;
  }

  .ai-video-section-header-{{ ai_gen_id }} {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 32px;
    flex-wrap: wrap;
    gap: 16px;
  }

  .ai-video-section-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.section_title_size }}px;
    font-weight: 700;
    color: {{ block.settings.text_color }};
    margin: 0;
  }

  .ai-video-section-button-{{ ai_gen_id }} {
    display: inline-flex;
    align-items: center;
    padding: 12px 24px;
    background-color: {{ block.settings.button_color }};
    color: {{ block.settings.button_text_color }};
    text-decoration: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    font-weight: 500;
    transition: background-color 0.3s ease;
  }

  .ai-video-section-button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_color }};
  }

  .ai-video-grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 24px;
  }

  .ai-video-item-{{ ai_gen_id }} {
    background-color: {{ block.settings.video_card_background }};
    border-radius: {{ block.settings.video_card_border_radius }}px;
    overflow: hidden;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);
    transition: transform 0.3s ease, box-shadow 0.3s ease;
  }

  .ai-video-item-{{ ai_gen_id }}:hover {
    transform: translateY(-4px);
    box-shadow: 0 4px 16px rgba(0, 0, 0, 0.15);
  }

  .ai-video-thumbnail-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    aspect-ratio: 16/9;
    background-color: #f0f0f0;
    cursor: pointer;
    overflow: hidden;
  }

  .ai-video-thumbnail-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-video-play-button-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 60px;
    height: 60px;
    background-color: rgba(0, 0, 0, 0.8);
    border: none;
    border-radius: 50%;
    color: white;
    cursor: pointer;
    transition: all 0.3s ease;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-video-play-button-{{ ai_gen_id }}:hover {
    background-color: rgba(0, 0, 0, 0.9);
    transform: translate(-50%, -50%) scale(1.1);
  }

  .ai-video-play-icon-{{ ai_gen_id }} {
    width: 24px;
    height: 24px;
    margin-left: 3px;
  }

  .ai-video-info-{{ ai_gen_id }} {
    padding: 16px;
  }

  .ai-video-title-{{ ai_gen_id }} {
    font-size:16px;
    font-weight: 600;
    color: {{ block.settings.text_color }};
    margin: 0 0 8px;line-height: 1.4;
  }

  .ai-video-creator-{{ ai_gen_id }} {
    font-size: 14px;
    color: {{ block.settings.creator_text_color }};
    margin: 0;
  }

  .ai-video-popup-{{ ai_gen_id }} {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.9);
    z-index: 1000;
    display: flex;
    align-items: center;
    justify-content: center;
    opacity: 0;
    visibility: hidden;
    transition: all 0.3s ease;
  }

  .ai-video-popup-{{ ai_gen_id }}.active {
    opacity: 1;
    visibility: visible;}

  .ai-video-popup-content-{{ ai_gen_id }} {
    position: relative;
    width: 90%;
    max-width: 900px;
    aspect-ratio: 16/9;
    background-color: #000;
  }

  .ai-video-popup-iframe-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    border: none;
  }

  .ai-video-popup-close-{{ ai_gen_id }} {
    position: absolute;
    top: -40px;
    right: 0;
    background: none;
    border: none;
    color: white;
    font-size: 24px;
    cursor: pointer;
    padding: 8px;
  }

  @media screen and (max-width: 768px) {
    .ai-video-nav-tabs-list-{{ ai_gen_id }} {
      padding: 0 16px;
    }

    .ai-video-nav-tab-{{ ai_gen_id }} {
      padding: 12px 16px;
      font-size: 14px;
    }

    .ai-video-sections-{{ ai_gen_id }} {
      padding: 24px 16px;
    }

    .ai-video-grid-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 16px;
    }

    .ai-video-section-header-{{ ai_gen_id }} {
      flex-direction: column;
      align-items: flex-start;
    }

    .ai-video-popup-content-{{ ai_gen_id }} {
      width: 95%;
    }

    .ai-video-popup-close-{{ ai_gen_id }} {
      top: -35px;
      font-size: 20px;
    }
  }

  @media screen and (min-width: 769px) and (max-width: 1024px) {
    .ai-video-grid-{{ ai_gen_id }} {
      grid-template-columns: repeat(2, 1fr);
    }
  }
{% endstyle %}

