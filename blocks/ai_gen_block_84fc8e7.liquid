    <section class="ai-product-section-{{ ai_gen_id }}" id="ranger-pro-{{ ai_gen_id }}">
      <div class="ai-product-section-header-{{ ai_gen_id }}">
        <h2 class="ai-product-section-title-{{ ai_gen_id }}">{{ block.settings.ranger_pro_title }}</h2>
        {% if block.settings.ranger_pro_link != blank %}
          <a href="{{ block.settings.ranger_pro_link }}" class="ai-product-view-btn-{{ ai_gen_id }}">View Product</a>
        {% endif %}
      </div>
      <div class="ai-product-videos-grid-{{ ai_gen_id }}">
        {% for i in (1..6) %}
          {% assign video_id_key = 'ranger_pro_video_' | append: i | append: '_id' %}
          {% assign video_title_key = 'ranger_pro_video_' | append: i | append: '_title' %}
          {% assign video_creator_key = 'ranger_pro_video_' | append: i | append: '_creator' %}
          {% assign video_id = block.settings[video_id_key] %}
          {% assign video_title = block.settings[video_title_key] %}
          {% assign video_creator = block.settings[video_creator_key] %}
          
          <div class="ai-product-video-item-{{ ai_gen_id }}" data-video-id="{{ video_id }}">
            <div class="ai-product-video-thumbnail-{{ ai_gen_id }}">
              {% if video_id != blank %}
                <img src="https://img.youtube.com/vi/{{ video_id }}/maxresdefault.jpg" alt="{{ video_title }}" loading="lazy">
                <button class="ai-product-video-play-btn-{{ ai_gen_id }}">
                  <svg viewBox="0 0 24 24">
                    <path d="M8 5v14l11-7z"/>
                  </svg>
                </button>
              {% else %}
                <div style="background: #f4f4f4; height: 100%; display: flex; align-items: center; justify-content: center; color: #999;">
                  Add YouTube Video ID
                </div>
              {% endif %}
            </div><div class="ai-product-video-content-{{ ai_gen_id }}">
              <h3 class="ai-product-video-title-{{ ai_gen_id }}">
                {% if video_title != blank %}{{ video_title }}{% else %}Video Title{% endif %}
              </h3>
              <p class="ai-product-video-creator-{{ ai_gen_id }}">
                {% if video_creator != blank %}{{ video_creator }}{% else %}Creator Name{% endif %}
              </p>
            </div>
          </div>
        {% endfor %}
      </div>
    </section>

