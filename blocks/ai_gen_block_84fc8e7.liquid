<script>
(function() {
  class ProductNavigation{{ ai_gen_id }} extends HTMLElement {
    constructor() {
      super();
      this.tabs = this.querySelectorAll('.ai-product-nav-tab-{{ ai_gen_id }}');
      this.sections = this.querySelectorAll('.ai-product-section-{{ ai_gen_id }}');
      this.scrollContainer = this.querySelector('.ai-product-nav-tabs-scroll-{{ ai_gen_id }}');
      this.scrollButtons = this.querySelectorAll('.ai-product-nav-scroll-btn-{{ ai_gen_id }}');
      this.videoItems = this.querySelectorAll('.ai-product-video-item-{{ ai_gen_id }}');
      this.modal = this.querySelector('.ai-product-video-modal-{{ ai_gen_id }}');
      this.modalIframe = this.querySelector('.ai-product-video-modal-iframe-{{ ai_gen_id }}');
      this.modalClose = this.querySelector('.ai-product-video-modal-close-{{ ai_gen_id }}');
    }

    connectedCallback() {
      this.setupTabNavigation();
      this.setupScrollButtons();
      this.setupScrollSpy();
      this.setupVideoModal();
      this.updateScrollButtons();
    }

    setupTabNavigation() {
      this.tabs.forEach(tab => {
        tab.addEventListener('click', () => {
          const target = tab.getAttribute('data-target');
          const section = this.querySelector(`#${target}-{{ ai_gen_id }}`);
          
          if (section) {
            section.scrollIntoView({ behavior: 'smooth' });
          }
        });
      });
    }

    setupScrollButtons() {
      this.scrollButtons.forEach(button => {
        button.addEventListener('click', () => {
          const direction = button.getAttribute('data-direction');
          const scrollAmount = 200;
          
          if (direction === 'left') {
            this.scrollContainer.scrollBy({ left: -scrollAmount, behavior: 'smooth' });
          } else {
            this.scrollContainer.scrollBy({ left: scrollAmount, behavior: 'smooth' });
          }
        });
      });

      this.scrollContainer.addEventListener('scroll', () => {
        this.updateScrollButtons();
      });
    }

    updateScrollButtons() {
      const { scrollLeft, scrollWidth, clientWidth } = this.scrollContainer;
      const leftButton = this.querySelector('.ai-product-nav-scroll-btn-{{ ai_gen_id }}.left');
      const rightButton = this.querySelector('.ai-product-nav-scroll-btn-{{ ai_gen_id }}.right');

      leftButton.style.opacity = scrollLeft > 0 ? '1' : '0.3';
      rightButton.style.opacity = scrollLeft < scrollWidth - clientWidth - 1 ? '1' : '0.3';
    }

    setupScrollSpy() {
      const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
          if (entry.isIntersecting && entry.intersectionRatio > 0.5) {
            const sectionId = entry.target.id.replace('-{{ ai_gen_id }}', '');
            this.updateActiveTab(sectionId);
          }
        });
      }, {
        threshold: 0.5,
        rootMargin: '-120px 0px -50% 0px'
      });

      this.sections.forEach(section => {
        observer.observe(section);
      });
    }

    updateActiveTab(sectionId) {
      this.tabs.forEach(tab => {
        tab.classList.remove('active');
        if (tab.getAttribute('data-target') === sectionId) {
          tab.classList.add('active');
        }
      });
    }

    setupVideoModal() {
      this.videoItems.forEach(item => {
        item.addEventListener('click', () => {
          const videoId = item.getAttribute('data-video-id');
          if (videoId) {
            this.openVideoModal(videoId);
          }
        });
      });

      this.modalClose.addEventListener('click', () => {
        this.closeVideoModal();
      });

      this.modal.addEventListener('click', (e) => {
        if (e.target === this.modal) {
          this.closeVideoModal();
        }
      });

      document.addEventListener('keydown', (e) => {
        if (e.key === 'Escape' && this.modal.classList.contains('active')) {
          this.closeVideoModal();
        }
      });
    }

    openVideoModal(videoId) {
      this.modalIframe.src = `https://www.youtube.com/embed/${videoId}?autoplay=1`;
      this.modal.classList.add('active');
      document.body.style.overflow = 'hidden';
    }

    closeVideoModal() {
      this.modal.classList.remove('active');
      this.modalIframe.src = '';
      document.body.style.overflow = '';
    }
  }

  customElements.define('product-navigation-{{ ai_gen_id }}', ProductNavigation{{ ai_gen_id }});
})();
</script>

