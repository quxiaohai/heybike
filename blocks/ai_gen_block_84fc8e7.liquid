        <button class="ai-product-nav-tab-{{ ai_gen_id }}" data-target="ranger-s">Ranger S</button>
        <button class="ai-product-nav-tab-{{ ai_gen_id }}" data-target="mars-2">Mars 2.0</button>
        <button class="ai-product-nav-tab-{{ ai_gen_id }}" data-target="horizon">Horizon</button>
        <button class="ai-product-nav-tab-{{ ai_gen_id }}" data-target="cityrun">Cityrun</button>
      </div>

      <button class="ai-product-nav-scroll-btn-{{ ai_gen_id }} right" data-direction="right">
        <svg viewBox="0 0 24 24">
          <path d="M9 18l6-6-6-6"/>
        </svg>
      </button>
    </div>
  </div>

