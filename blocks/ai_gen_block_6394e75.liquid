                  src="https://img.youtube.com/vi/{{ video_id }}/maxresdefault.jpg"
                  alt="{{ video_title | escape }}"
                  class="ai-video-thumbnail-image-{{ ai_gen_id }}"
                  loading="lazy"
                >
                <button class="ai-video-play-button-{{ ai_gen_id }}" aria-label="Play video">
                  <svg class="ai-video-play-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="currentColor">
                    <path d="M8 5v14l11-7z"/>
                  </svg>
                </button>
              </div>
              <div class="ai-video-info-{{ ai_gen_id }}">
                {% if video_title != blank %}
                  <h3 class="ai-video-title-{{ ai_gen_id }}">{{ video_title }}</h3>
                {% endif %}
                {% if video_creator != blank %}
                  <p class="ai-video-creator-{{ ai_gen_id }}">{{ video_creator }}</p>
                {% endif %}
              </div>
            </div>
          {% endif %}
        {% endfor %}
      </div>
    </section>
