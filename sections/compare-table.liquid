{{- 'compare-table.css' | asset_url | stylesheet_tag -}}
{%- liquid
  assign section_heading = section.settings.heading
-%}
<compare-table class="compare-table section section--padding">
  <div class="row relative">
      <header class="section-header">
        {% if section_heading != blank %}
        <h1 class="section-header__title compare">{{ section_heading }}</h1>
        {% endif %}
      </header>
      <ul class="compare-table-tab">
      {% if section.blocks.size > 1 %}
        {%- for block in section.blocks -%}
          {%- assign mainloop = forloop -%}
          {% if block.settings.product_list != blank %}
            <li class="{% if mainloop.index0 == 0 %} active {% endif %} " data-index="{{ mainloop.index0 }}">
              {{ block.settings.title }}
            </li>
          {% endif %}
        {%- endfor -%}
      {% endif %}
      </ul>
  </div>

  <div class="compare-table-tab-contents row relative">
    {%- for block in section.blocks -%}
      {%- assign mainloop = forloop -%}
      {% if block.settings.product_list != blank %}
        <div
          class="tab-content-item {% if mainloop.index0 == 0 %} active {% endif %}"
          data-index="{{ mainloop.index0 }}"
        >
          <div class="fixed-content sticky-navigation">
            <div class="tab-product-detail-fixed">
              {%- for i in (1..3) -%}
                <div class="product-select  {% if i ==3  %} compare-table-mobile {% endif %}">
                  <select data-index="{{ i | minus: 1  }}">
                    {% if block.settings.product_list_group != blank%}
                    <optgroup label="{{ block.settings.product_list_group }}">
                    {% endif %}
                    {% for product in block.settings.product_list %}
                      <option
                        data-featured-image="{{product.featured_image | image_url}}"
                        data-url="{{product.url}}"
                        data-price="{{product.price | money}}"
                        value="{{product.handle}}"
                        {% if forloop.index == i %}
                          selected
                        {% endif %}
                      >
                        {{ product.title }}
                      </option>
                    {% endfor %}
                    {% if block.settings.product_list_group != blank %}
                    </optgroup>
                    {% endif %}
                    {% if block.settings.product_list_group_1 != blank %}
                    <optgroup label="{{ block.settings.product_list_group_1 }}">
                    {% endif %}
                    {% for product in block.settings.product_list_1 %}
                      <option
                        data-featured-image="{{product.featured_image | image_url}}"
                        data-url="{{product.url}}"
                        data-price="{{product.price | money}}"
                        value="{{product.handle}}"
                      >
                        {{ product.title }}
                      </option>
                    {% endfor %}
                    {% if block.settings.product_list_group_1 != blank %}
                    </optgroup>
                    {% endif %}
                    
                    {% if block.settings.product_list_group_2 != blank %}
                    <optgroup label="{{ block.settings.product_list_group_2 }}">
                    {% endif %}
                    {% for product in block.settings.product_list_2 %}
                      <option
                        data-featured-image="{{product.featured_image | image_url}}"
                        data-url="{{product.url}}"
                        data-price="{{product.price | money}}"
                        value="{{product.handle}}"
                      >
                        {{ product.title }}
                      </option>
                    {% endfor %}
                    {% if block.settings.product_list_group_2 != blank %}
                    </optgroup>
                    {% endif %}
                    
                    {% if block.settings.product_list_group_3 != blank %}
                    <optgroup label="{{ block.settings.product_list_group_3 }}">
                    {% endif %}
                    {% for product in block.settings.product_list_3 %}
                      <option
                        data-featured-image="{{product.featured_image | image_url}}"
                        data-url="{{product.url}}"
                        data-price="{{product.price | money}}"
                        value="{{product.handle}}"
                      >
                        {{ product.title }}
                      </option>
                    {% endfor %}
                    {% if block.settings.product_list_group_3 != blank %}
                    </optgroup>
                    {% endif %}
                    
                    {% if block.settings.product_list_group_4 != blank %}
                    <optgroup label="{{ block.settings.product_list_group_4 }}">
                    {% endif %}
                    {% for product in block.settings.product_list_4 %}
                      <option
                        data-featured-image="{{product.featured_image | image_url}}"
                        data-url="{{product.url}}"
                        data-price="{{product.price | money}}"
                        value="{{product.handle}}"
                      >
                        {{ product.title }}
                      </option>
                    {% endfor %}
                    {% if block.settings.product_list_group_4 != blank %}
                    </optgroup>
                    {% endif %}
                    
                    {% if block.settings.product_list_group_5 != blank%}
                    <optgroup label="{{ block.settings.product_list_group_5 }}">
                    {% endif %}
                    {% for product in block.settings.product_list_5 %}
                      <option
                        data-featured-image="{{product.featured_image | image_url}}"
                        data-url="{{product.url}}"
                        data-price="{{product.price | money}}"
                        value="{{product.handle}}"
                      >
                        {{ product.title }}
                      </option>
                    {% endfor %}
                    {% if block.settings.product_list_group_5 != blank%}
                    </optgroup>
                    {% endif %}
                    
                    {% if block.settings.product_list_group_6 != blank %}
                    <optgroup label="{{ block.settings.product_list_group_6 }}">
                    {% endif %}
                    {% for product in block.settings.product_list_6 %}
                      <option
                        data-featured-image="{{product.featured_image | image_url}}"
                        data-url="{{product.url}}"
                        data-price="{{product.price | money}}"
                        value="{{product.handle}}"
                      >
                        {{ product.title }}
                      </option>
                    {% endfor %}
                    {% if block.settings.product_list_group_6 != blank %}
                    </optgroup>
                    {% endif %}
                    
                    {% if block.settings.product_list_group_7 != blank%}
                    <optgroup label="{{ block.settings.product_list_group_7 }}">
                    {% endif %}
                    {% for product in block.settings.product_list_7 %}
                      <option
                        data-featured-image="{{product.featured_image | image_url}}"
                        data-url="{{product.url}}"
                        data-price="{{product.price | money}}"
                        value="{{product.handle}}"
                      >
                        {{ product.title }}
                      </option>
                    {% endfor %}
                    {% if block.settings.product_list_group_7 != blank%}
                    </optgroup>
                    {% endif %}
                  </select>
                  <label>Model</label>
                </div>
              {% endfor %}
            </div>
          </div>
          <div class="small-12 tab-product-details">
            {%- for i in (1..3) -%}
              <div class="tab-product-detail {% if i ==3  %} compare-table-mobile {% endif %}">
                <div>
                  <div class="product-select mb16">
                  <select data-index="{{ i | minus: 1  }}">
                     {% if block.settings.product_list_group != blank%}
                    <optgroup label="{{ block.settings.product_list_group }}">
                    {% endif %}
                    {% for product in block.settings.product_list %}
                      <option
                        data-featured-image="{{product.featured_image | image_url}}"
                        data-url="{{product.url}}"
                        data-price="{{product.price | money}}"
                        value="{{product.handle}}"
                        {% if forloop.index == i %}
