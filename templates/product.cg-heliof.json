    "cg_product_feature_list_WxXtr9": {
      "type": "cg_product-feature-list",
      "blocks": {
        "feature_9b3zEb": {
          "type": "feature",
          "settings": {
            "feature_img": "shopify://shop_images/Helio_F_Handle_Bar.png",
            "feature_heading": "Shimano 8-Speed Gear System",
            "feature_desc": "<p>The Shimano Altus 8-speed drivetrain ensures smooth, precise, and reliable shifting—whether you’re cruising through the city, tackling gentle climbs, or riding longer distances. Perfect for riders who value both control and comfort.</p>"
          }
        },
        "feature_fVDXQH": {
          "type": "feature",
          "settings": {
            "feature_img": "shopify://shop_images/Helio_F_Seatpost.png",
            "feature_heading": "Removable Battery",
            "feature_desc": "<p>The 360Wh battery delivers a range of 80–120 km and can be easily removed for convenient charging anywhere—at home, at the office, or on the go. No need to move your bike just to recharge.</p>"
          }
        },
        "feature_hHWBPT": {
          "type": "feature",
          "settings": {
            "feature_img": "shopify://shop_images/Helio_F_Torque.png",
            "feature_heading": "Torque Sensor",
            "feature_desc": "<p>The torque sensor detects how much pressure you put on the pedals and adjusts the motor support accordingly. This makes every ride more efficient, more natural, and simply more enjoyable.</p>"
          }
        },
        "feature_q7H889": {
          "type": "feature",
          "settings": {
            "feature_img": "shopify://shop_images/air_tag.png",
            "feature_heading": "AirTag-Tracking",
            "feature_desc": "<p>With the AirTag, you can track your Helio F at any time. Whether in the city, while traveling, or in daily life — an Apple AirTag adds extra security and ensures your bike is always within reach.</p>"
          }
        },
        "feature_mzDqYy": {
          "type": "feature",
          "settings": {
            "feature_img": "shopify://shop_images/Helio_F_Front_Tire.png",
            "feature_heading": "Premium Schwalbe Tires",
            "feature_desc": "<p>Equipped with Schwalbe tires for excellent grip, superior comfort, and long-lasting performance. From cobblestone streets to narrow alleys or light trails, your ride stays smooth, stable, and secure.</p>"
          }
        },
        "feature_ybC4Dj": {
          "type": "feature",
          "settings": {
            "feature_img": "shopify://shop_images/Helio_F_Headlight.png",
            "feature_heading": "Integrated Headlight",
            "feature_desc": "<p>The built-in headlight keeps your path illuminated, ensuring safety and visibility day and night. Dark streets and unexpected obstacles are no longer a concern.</p>"
          }
        }
