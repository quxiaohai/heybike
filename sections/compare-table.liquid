                        {% endif %}
                      >
                        {{ product.title }}
                      </option>
                    {% endfor %}
                    {% if block.settings.product_list_group != blank %}
                    </optgroup>
                    {% endif %}
                    {% if block.settings.product_list_group_1 != blank %}
                    <optgroup label="{{ block.settings.product_list_group_1 }}">
                    {% endif %}
                    {% for product in block.settings.product_list_1 %}
                      <option
                        data-featured-image="{{product.featured_image | image_url}}"
                        data-url="{{product.url}}"
